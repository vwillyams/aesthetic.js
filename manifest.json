{
  "manifest_version": 2,

  "name": "Aesthetic.js",
  "description": "Make everything A E S T H E T I C",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["aesthetic.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "clipboardWrite",
    "clipboardRead"
  ]
}